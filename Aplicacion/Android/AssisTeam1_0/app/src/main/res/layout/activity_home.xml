<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_home"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.example.fenix.assisteam1_0.Home"
    android:weightSum="1">

    <Button
        android:id="@+id/idHomeButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="90dp"
        android:text="@string/name_contratarServicios"
        android:layout_weight="0.16" />

    <Button
        android:id="@+id/idHomeButton1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/name_contratarServicios"
        android:layout_weight="0.16"
        android:layout_alignBaseline="@+id/idHomeButton"
        android:layout_alignBottom="@+id/idHomeButton"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />
</RelativeLayout>
